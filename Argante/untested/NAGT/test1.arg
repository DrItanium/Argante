!SIGNATURE	"Linker test"
!PRIORITY	0x2710

.DATA
:string1_1
	"Local string, local code.\n"
:string1_2
	"Local string, external code.\n"
.CODE
:code1_1
	twait   0x7d0
	mov   u0,:string1_1
	mov   u1,^string1_1
	syscall   $IO_PUTSTRING
	call :code2_1
	halt
:code1_2
	mov   u0,:string2_1
	mov   u1,^string2_1
	syscall   $IO_PUTSTRING
	ret 0

