!SIGNATURE	"Argante v2 test"
!PRIORITY	0x2710

# Some various data items with Endianness data
.DATA
:string
	"Local string, external code.\n"
:int_array
	s:10000
	u:010000
	u:0x10101
	u:"FOOF"
:float_array
	0.12
	0.121
	0.1212
	f:1202004
{
.DATA
:fake
# CR-LF, as a string
	t:0x1310
.CODE
	mov	u:r1, s:100
	mov	u:r0, 10000
	add	u:r0, u:r1
	mov	u:r1, *u::fake
	add	s:r0, s:r1
	mov	f:r1, 0.001
	mul	f:r1, s:r0
	div	f:r1, *f:%float_array
	mov	u:r0, f:r1
	sub	u:r0, *u:^fake
	mul	*u::fake, :non_existent_addy
	halt
}
